<%- include('includes/header') %>

<!-- Page Content -->
<div id="page-content-wrapper" style="min-height:100%">
    <div class="container-fluid">
        <div class="row">
            <div class="col-lg-12">
                <div class="row">
                    <div class="col-sm-12" style="margin-bottom:10">
                        <input class="form-control" id="myInput" type="text" placeholder="Search..">
                    </div>
                    <div class="col-sm-12 col-md-12 col-lg-12">
                        <table class="table table-striped table-bordered table-sm" cellspacing="0" width="100%">
                            <thead class="thead-dark">
                                <tr>
                                    <th class="th-sm">Region
                                    </th>
                                    <th class="th-sm">Municipality
                                    </th>
                                    <th class="th-sm">Neighborhood
                                    </th>
                                    <th class="th-sm">Street
                                    </th>
                                    <th class="th-sm">Building
                                    </th>
                                    <th class="th-sm">Latitude
                                    </th>
                                    <th class="th-sm">Longitude
                                    </th>
                                </tr>
                            </thead>
                            <tbody id="myTable">
                                <% for (var i = 0; i < addresses.length; i++) { %>
                                <tr>
                                    <td><%= addresses[i].region_name %></td>
                                    <td><%= addresses[i].municipality_name %></td>
                                    <td><%= addresses[i].neighborhood %></td>
                                    <td><%= addresses[i].street_country %></td>
                                    <td><%= addresses[i].object_id %></td>
                                    <td><%= addresses[i].latitude %></td>
                                    <td><%= addresses[i].longitude %></td>
                                </tr>
                                <% } %>
                            </tbody>
                        </table>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>
<!-- /#page-content-wrapper -->
<footer>
    <p><strong>OS:</strong> <%= javaData.OS %></p>
    <p><strong>TIME:</strong> <%= (new Date(parseInt(javaData.time))) %></p>
</footer>

<%- include('includes/footer') %>